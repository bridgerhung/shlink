{
  "functions": {
    "api/invalid-short-code.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/maintenance",
      "dest": "/maintenance.html"
    },
    {
      "src": "/invalid-short-code",
      "dest": "/api/invalid-short-code"
    },
    {
      "src": "/(.*)",
      "dest": "/404.html"
    }
  ]
}